{
  "name": "site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbopack",
    "build": "next build",
    "export": "next build",
    "deploy": "npm run build && touch out/.nojekyll",
    "gh-deploy": "node deploy-gh-pages.js",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test:email": "curl -X POST http://localhost:3000/api/email/test -H 'Content-Type: application/json' -H 'Authorization: Bearer your-token' -d '{\"to\":\"test@example.com\"}'",
    "health-check": "curl http://localhost:3000/api/health",
    "analyze": "node scripts/optimize.js",
    "optimize": "npm run analyze && echo 'Check optimization-report.json for detailed analysis'"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.534.0",
    "next": "15.4.5",
    "nodemailer": "^6.9.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.61.1",
    "react-hot-toast": "^2.5.2",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "gh-pages": "^6.3.0",
    "tailwindcss": "^4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
